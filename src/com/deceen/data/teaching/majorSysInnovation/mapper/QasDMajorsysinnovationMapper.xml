<mapper namespace="com.deceen.data.teaching.majorSysInnovation.mapper.QasDMajorsysinnovationMapper">
<!--  ********************************QAS_D_MAJORSYSINNOVATION config begnin********************************  -->

<select id="select" resultType="QasDMajorsysinnovation"> 
 SELECT ID,SUBJECTNAME,MAININNOVATION,IMPLEMENTATIONDATE,MAINACHIEVEMENT,STATUS,CREATEDBY,CREATEDTIME,UPDATEDBY,UPDATEDTIME,TENANTID,STORAGETIME FROM  QAS_D_MAJORSYSINNOVATION 
 WHERE   ID=#{id}
</select>

<select id="list" parameterType="QasDMajorsysinnovation" useCache="false"   resultType ="hashmap" >  
 <include refid="plat.ORACLE_PAGE_HEADER" />
 SELECT ID,SUBJECTNAME,MAININNOVATION,IMPLEMENTATIONDATE,MAINACHIEVEMENT,STATUS,CREATEDBY,CREATEDTIME,UPDATEDBY,UPDATEDTIME,TENANTID,STORAGETIME FROM  QAS_D_MAJORSYSINNOVATION 
 WHERE  1 = 1
 <!-- 
   ## 这里为查询的样例 修改完必须删除此些内容
 <if test="filed != null and filed != ''">
 AND COLUMN LIKE '%' #{filed}  '%'
 </if>
  <if test="endTime != null and endTime != ''">
      	<![CDATA[  AND  #{endTime} >= DATE_FORMAT(COLUMN,'%Y-%m-%d')]]>
  </if>
 -->
 <include refid="plat.ORACLE_PAGE_FOOTER" />
</select>

 <insert id="insert" parameterType="QasDMajorsysinnovation">
 INSERT INTO QAS_D_MAJORSYSINNOVATION   ( ID,SUBJECTNAME,MAININNOVATION,IMPLEMENTATIONDATE,MAINACHIEVEMENT,STATUS,CREATEDBY,CREATEDTIME,UPDATEDBY,UPDATEDTIME,TENANTID,STORAGETIME)
  VALUES ( #{id},#{subjectname},#{maininnovation},#{implementationdate},#{mainachievement},#{status},#{createdby},#{createdtime},#{updatedby},#{updatedtime},#{tenantid},#{storagetime} )
</insert>

 <update id="update" parameterType="QasDMajorsysinnovation">
 UPDATE QAS_D_MAJORSYSINNOVATION SET  ID=#{id},SUBJECTNAME=#{subjectname},MAININNOVATION=#{maininnovation},IMPLEMENTATIONDATE=#{implementationdate},MAINACHIEVEMENT=#{mainachievement},STATUS=#{status},CREATEDBY=#{createdby},CREATEDTIME=#{createdtime},UPDATEDBY=#{updatedby},UPDATEDTIME=#{updatedtime},TENANTID=#{tenantid},STORAGETIME=#{storagetime} 
 WHERE  ID=#{id}
</update>


 <delete id="delete">
   DELETE FROM QAS_D_MAJORSYSINNOVATION WHERE   ID=#{id}
</delete>

<!-- 批量删除  -->
    <delete id="batchDelete">
         DELETE FROM QAS_D_MAJORSYSINNOVATION WHERE ID IN
         <foreach collection="array" item="id" separator="," open="(" close=")">
            #{id}
        </foreach>
    </delete>
<!-- 动态进行更新 如果有需要请开启，否则请删除 -->
 <update id="updateWithDyna" parameterType="QasDMajorsysinnovation">
 UPDATE QAS_D_MAJORSYSINNOVATION 
 <trim prefix="SET" suffixOverrides=",">
 <if test="id != null">  ID = #{id},</if>
<if test="subjectname != null">  SUBJECTNAME = #{subjectname},</if>
<if test="maininnovation != null">  MAININNOVATION = #{maininnovation},</if>
<if test="implementationdate != null">  IMPLEMENTATIONDATE = #{implementationdate},</if>
<if test="mainachievement != null">  MAINACHIEVEMENT = #{mainachievement},</if>
<if test="status != null">  STATUS = #{status},</if>
<if test="createdby != null">  CREATEDBY = #{createdby},</if>
<if test="createdtime != null">  CREATEDTIME = #{createdtime},</if>
<if test="updatedby != null">  UPDATEDBY = #{updatedby},</if>
<if test="updatedtime != null">  UPDATEDTIME = #{updatedtime},</if>
<if test="tenantid != null">  TENANTID = #{tenantid},</if>
<if test="storagetime != null">  STORAGETIME = #{storagetime},</if>

 </trim>  
 WHERE  ID=#{id}
</update>

<!--  ********************************QAS_D_MAJORSYSINNOVATION config end********************************  -->

</mapper>
