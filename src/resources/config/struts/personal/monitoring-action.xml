<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="monitoring" extends="default" namespace="/personal/monitoring">
	
		<action name="query" class="monitoringAction" method="query">
			<result name="select">/pages/personal/monitoring/monitoring.jsp</result>
		</action>
		
		<action name="barChart" class="monitoringAction" method="barChart"/>
		
		<action name="barcharChange" class="monitoringAction" method="barcharChange"/>
		
				
		<action name="details" class="monitoringAction" method="details">
		<result name="list">/pages/personal/monitoring/details.jsp</result>
		</action>
		
		<action name="detailsVs" class="monitoringAction" method="detailsVs">
		<result name="list">/pages/personal/monitoring/details.jsp</result>
		</action>
		
		
	</package>
</struts>
